{
  "name": "QMask",
  "short_name": "QMask",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "Nussbaumer Projects",
  "description": "Qtum Browser Extension",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Alt+Shift+Q",
        "mac": "Alt+Shift+Q",
        "chromeos": "Alt+Shift+Q",
        "linux": "Alt+Shift+Q"
      }
    }
  },
  "icons": {
    "16": "images/logo_16.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  },
  "background" : {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "48": "images/logo_48.png",
    "128": "images/logo_128.png"
    },
    "default_title": "QMask",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "file://*/*",
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/contentscript.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": [
    "storage",
    "clipboardWrite",
    "http://localhost:8545/"
    ]
}
